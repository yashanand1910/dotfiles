#!/bin/bash -e

git pull
git add .
git commit -m "Update config on $(date)"
git push

docker build -t yashanand1910/dev -t dev .
docker build -t yashanand1910/dev-private -t dev-private -f private.Dockerfile .

docker push yashanand1910/dev:latest
docker push yashanand1910/dev-private:latest

docker system prune -f --volumes
